#include<stdio.h>
int main()
{
	int n,num,temp,size,digit[20],i=0;
	scanf("%d%d",&num,&n);
	temp=num;
	while(num>0)
	{
	       digit[i]=temp%10;
	       temp/=10;
	       i++;
	}
	size=i;
	for(i=n;i<size;i++)
	{
		printf("%d",digit[i]);
	}
	for(i=0;i<n;i++)
	{
		printf("%d",digit[i]);
	}
	return 0;
}
